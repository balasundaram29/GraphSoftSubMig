<?xml version="1.0"?>
 <!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ObsValuesOfReadings" table="OBSERVED">
        <id name="id" type="int" column="OID" >
            <generator class="increment"/>
        </id>
        <property name="readingSNo">
            <column name="READINGSNO"   />
        </property>

        <property name="discharge">
            <column name="DISCHARGE" />
        </property>
        <property name="head">
            <column name="HEAD"/>
        </property>
        <property name="eff">
            <column name="EFF"/>
        </property>
        <property name="current">
            <column name="MCURRENT" />
        </property>
    </class>
</hibernate-mapping>